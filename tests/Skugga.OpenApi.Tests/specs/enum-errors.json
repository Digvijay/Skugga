{
  "openapi": "3.0.0",
  "info": {
    "title": "Enum Validation Errors Test API",
    "version": "1.0.0"
  },
  "paths": {
    "/items": {
      "get": {
        "operationId": "getItems",
        "summary": "Get items with invalid enum example",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "required": false,
            "description": "Filter by status",
            "schema": {
              "type": "string",
              "enum": ["active", "inactive"]
            },
            "example": "invalid_status"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Item"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Item": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "example": 1
          },
          "status": {
            "type": "string",
            "enum": ["draft", "published", "archived"],
            "example": "invalid_example"
          },
          "priority": {
            "type": "integer",
            "enum": [1, 2, 3],
            "example": "1"
          }
        }
      }
    }
  }
}
